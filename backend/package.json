{
  "name": "stream-camera-backend",
  "version": "1.0.0",
  "description": "Backend API for managing go2rtc camera configuration",
  "main": "server.js",
  "scripts": {
    "start": "concurrently -n \"API,go2rtc\" -c \"blue,green\" \"node server.js\" \".\\go2rtc.exe -c go2rtc.yaml\"",
    "dev": "concurrently -n \"API,go2rtc\" -c \"blue,green\" \"nodemon server.js\" \".\\go2rtc.exe -c go2rtc.yaml\"",
    "api": "node server.js",
    "go2rtc": ".\\go2rtc.exe -c go2rtc.yaml"
  },
  "keywords": [
    "go2rtc",
    "camera",
    "streaming"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
